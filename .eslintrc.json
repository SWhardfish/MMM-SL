{
	"extends": "eslint:recommended",
	"globals": {
		"Module": false,
		"Log": false
	},
	"rules": {
		"indent": ["error", 2],
		"quotes": ["error", "double"],
		"max-len": ["error", 250],
		"curly": "error",
		"camelcase": ["error", {"properties": "never"}],
		"no-trailing-spaces": ["error", {"skipBlankLines": true}],
		"no-irregular-whitespace": ["error"],
		"no-extra-semi": "error",
		"no-console": 0,
		"no-unused-vars": ["error", { "args": "none" }]
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	}
}
